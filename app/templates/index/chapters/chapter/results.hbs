<div class="container">
    <div class="row">
        <div class="col-xs-12 col-md-offset-3 col-md-6 col-md-offset-3">
            <h1 style="color: #ffffff">Thank you for taking the time to take this survey!</h1>

          {{#link-to 'index.chapters.chapter.welcome' model.chapter.id class="btn-link"}}
              <button type="button" {{action "restart" model.member model.chapter}} class=" btn btn-default btn-lg btn-block" style="margin-top: 25px; margin-bottom: 30px;">Retake Survey</button>
          {{/link-to}}
        </div>
    </div>
</div>

<div style="background: #ffffff">
    <div class="container" style="padding-left: 0; padding-right: 0">
        <div class="row">
            <div class="col-xs-12 col-md-offset-3 col-md-6 col-md-offset-3">
              {{member-consequences member=model.member}}
            </div>
        </div>
    </div>
</div>
